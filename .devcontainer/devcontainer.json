{
	// TODO check if alternatively macOS can be supported as well.
	"name": "Debian",	
	"image": "mcr.microsoft.com/devcontainers/base:bullseye",
    
"features": {
    "ghcr.io/devcontainers/features/sshd:1": {
        "version": "latest"
    }
},
"postAttachCommand": "unset GITHUB_TOKEN;",

"remoteEnv": {		
		 "JHLP_SETUP_GITHUB_CLI": "1",
		"JHLP_NO_MULTIPLEXER": "1"
	}
}
